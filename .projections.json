{
    "app/api/*.py": {
        "alternate": "test/{}_test.py",
        "type": "api"
    },
    "test/*_test.py": {
        "alternate": "app/api/{}.py",
        "type": "test"
    },
    "*.py": {
        "console": "poetry run ipython"
    },
    "*": {
        "dispatch": "poetry run pytest",
        "make": "poetry build"
    }
}
